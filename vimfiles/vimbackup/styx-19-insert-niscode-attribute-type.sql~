DECLARE
AREA_TYPE_ID NUMBER(20,0);
ATTRIBUTE_TYPE_ID NUMBER(20,0);
BEGIN
    -- INSERT COUNTRY_NISCODE
    SELECT id INTO AREA_TYPE_ID FROM AREA_TYPE WHERE CODE = 'COUNTRY';
    ATTRIBUTE_TYPE_ID := SEQ_ATTRIBUTETYPE.NEXTVAL;

    INSERT
    INTO ATTRIBUTE_TYPE ( ID, CODE, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, UNIQUE_VALUE, VERSION
    )
    VALUES ( ATTRIBUTE_TYPE_ID, 'COUNTRY_NISCODE', SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1, 1);

    INSERT
    INTO ATYPE_ATTYPE (AREA_TYPE_ID, ATTRIBUTE_TYPE_ID)
    VALUES(AREA_TYPE_ID, ATTRIBUTE_TYPE_ID);

    -- INSERT CITY_NISCODE
    SELECT id INTO AREA_TYPE_ID FROM AREA_TYPE WHERE CODE = 'CITY';
    ATTRIBUTE_TYPE_ID := SEQ_ATTRIBUTETYPE.NEXTVAL;

    INSERT
    INTO ATTRIBUTE_TYPE ( ID, CODE, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, UNIQUE_VALUE, VERSION
    )
    VALUES ( ATTRIBUTE_TYPE_ID, 'CITY_NISCODE', SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1, 1);

    INSERT
    INTO ATYPE_ATTYPE (AREA_TYPE_ID, ATTRIBUTE_TYPE_ID)
    VALUES(AREA_TYPE_ID, ATTRIBUTE_TYPE_ID);

    -- INSERT CITY_NISCODE
    ATTRIBUTE_TYPE_ID := SEQ_ATTRIBUTETYPE.NEXTVAL;

    INSERT
    INTO ATTRIBUTE_TYPE ( ID, CODE, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, UNIQUE_VALUE, VERSION
    )
    VALUES ( ATTRIBUTE_TYPE_ID, 'CITY_CRAB_ID', SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1, 1);

    INSERT
    INTO ATYPE_ATTYPE (AREA_TYPE_ID, ATTRIBUTE_TYPE_ID)
    VALUES(AREA_TYPE_ID, ATTRIBUTE_TYPE_ID);

    -- INSERT CITY_NISCODE 
    SELECT id INTO AREA_TYPE_ID FROM AREA_TYPE WHERE CODE = 'STREET';
    ATTRIBUTE_TYPE_ID := SEQ_ATTRIBUTETYPE.NEXTVAL;

    INSERT
    INTO ATTRIBUTE_TYPE ( ID, CODE, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, UNIQUE_VALUE, VERSION
    )
    VALUES ( ATTRIBUTE_TYPE_ID, 'STREET_CRAB_ID', SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1, 1);

    INSERT
    INTO ATYPE_ATTYPE (AREA_TYPE_ID, ATTRIBUTE_TYPE_ID)
    VALUES(AREA_TYPE_ID, ATTRIBUTE_TYPE_ID);

    -- INSERT CITY_NISCODE 
    SELECT id INTO AREA_TYPE_ID FROM AREA_TYPE WHERE CODE = 'HOUSENUMBER';
    ATTRIBUTE_TYPE_ID := SEQ_ATTRIBUTETYPE.NEXTVAL;

    INSERT
    INTO ATTRIBUTE_TYPE ( ID, CODE, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, UNIQUE_VALUE, VERSION
    )
    VALUES ( ATTRIBUTE_TYPE_ID, 'HOUSENUMBER_CRAB_ID', SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1, 1);

    INSERT
    INTO ATYPE_ATTYPE (AREA_TYPE_ID, ATTRIBUTE_TYPE_ID)
    VALUES(AREA_TYPE_ID, ATTRIBUTE_TYPE_ID);


    COMMIT;

END;
GO
