    INSERT INTO STATUS_CODE ( ID, CODE, CREATION_DATE_SOURCE, CREATION_USER_SOURCE, STATUS_TYPE_ID, DT_ADD, DT_UPDATE, USER_ADD, USER_UPDATE, VERSION)
    VALUES ( SEQ_STATUS_CODE.NEXTVAL, '1',  SYSDATE, 'Styx', STATUS_TYPE_ID, SYSDATE, SYSDATE, 'noPrincipal', 'noPrincipal', 1);


    INSERT INTO STATUS_CODE ( ID, CODE, CREATION_DATE_SOURCE, CREATION_USER_SOURCE, STATUS_TYPE_ID, DT_ADD, DT_UPDATE, USER_ADD, USER_UPDATE, VERSION)
    VALUES ( SEQ_STATUS_CODE.NEXTVAL, '2',  SYSDATE, 'Styx', STATUS_TYPE_ID, SYSDATE, SYSDATE, 'noPrincipal', 'noPrincipal', 1);


    INSERT INTO STATUS_CODE ( ID, CODE, CREATION_DATE_SOURCE, CREATION_USER_SOURCE, STATUS_TYPE_ID, DT_ADD, DT_UPDATE, USER_ADD, USER_UPDATE, VERSION)
    VALUES ( SEQ_STATUS_CODE.NEXTVAL, '3',  SYSDATE, 'Styx', STATUS_TYPE_ID, SYSDATE, SYSDATE, 'noPrincipal', 'noPrincipal', 1);


