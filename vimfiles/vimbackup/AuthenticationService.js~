(function(){
    'use strict';

    angular.module('vatbook.services').factory('Authentication', function(){
        var Authentication = {
            register: register;
        };

        return Authentication;

        function register(email, password, username) {
            return $http.post('/api/v1/accounts/', {
                username: username,
                password: password,
                email: email

            });
        }

    });
}());
