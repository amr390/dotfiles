DECLARE
AREA_TYPE_ID NUMBER(20,0);
STATUS_TYPE_ID NUMBER(20,0);
BEGIN
     -- INSERT STREET_STATUS_TYPE status
    SELECT id INTO AREA_TYPE_ID FROM AREA_TYPE WHERE CODE = 'STREET';
    STATUS_TYPE_ID := SEQ_STATUS_TYPE.NEXTVAL;

    INSERT
    INTO STATUS_TYPE ( ID, CODE, AREA_TYPE_ID, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, VERSION
    )
    VALUES ( STATUS_TYPE_ID, 'STREET_STATUS_TYPE', AREA_TYPE_ID, SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1);

   -- INSERT HOUSENUMBER_STATUS_TYPE status
    SELECT id INTO AREA_TYPE_ID FROM AREA_TYPE WHERE CODE = 'HOUSENUMBER';
    STATUS_TYPE_ID := SEQ_STATUS_TYPE.NEXTVAL;

    INSERT
    INTO STATUS_TYPE ( ID, CODE, AREA_TYPE_ID, DT_ADD, DT_UPDATE, REQUIRED,
        USER_ADD, USER_UPDATE, VERSION
    )
    VALUES ( STATUS_TYPE_ID, 'HOUSENUMBER_STATUS_TYPE', AREA_TYPE_ID, SYSDATE,
        SYSDATE, 1, 'noPrincipal', 'noPrincipal', 1);

    COMMIT;

END;
GO
