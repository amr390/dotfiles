DECLARE
    AREA_TYPE_NAME_ID NUMBER;
    AREA_TYPE_DESCRIPTION_ID NUMBER;
    COUNTRY_AREA_TYPE_NAME_ID NUMBER;
	AREA_ID NUMBER;
BEGIN
    SELECT SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL INTO AREA_TYPE_NAME_ID FROM DUAL;
    SELECT SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL INTO AREA_TYPE_DESCRIPTION_ID FROM DUAL;

	insert into MULTILANGUAGE_PROVIDER (ID, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(AREA_TYPE_NAME_ID, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);
	insert into MULTILANGUAGE (ID, LABEL, LANGUAGE_ID, MULTILANGUAGE_PROVIDER_ID, START_DATE, END_DATE, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(SEQ_MULTILANGUAGE.NEXTVAL, 'Country', 4, AREA_TYPE_NAME_ID, SYSDATE, null, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);

	insert into MULTILANGUAGE_PROVIDER (ID, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(AREA_TYPE_DESCRIPTION_ID, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);
	insert into MULTILANGUAGE (ID, LABEL, LANGUAGE_ID, MULTILANGUAGE_PROVIDER_ID, START_DATE, END_DATE, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(SEQ_MULTILANGUAGE.NEXTVAL, 'Country area type', 4, AREA_TYPE_DESCRIPTION_ID, SYSDATE, null, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);
	insert into AREA_TYPE (ID, CODE, NAME_PROVIDER_ID, DESCRIPTION_PROVIDER_ID, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(SEQ_AREA_TYPE.NEXTVAL, 'COUNTRY', AREA_TYPE_NAME_ID, AREA_TYPE_DESCRIPTION_ID, 1, 'insert', SYSDATE, 'update', SYSDATE);


    SELECT SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL INTO COUNTRY_AREA_TYPE_NAME_ID FROM DUAL;
    SELECT SEQ_AREA.NEXTVAL INTO AREA_ID FROM DUAL;

	insert into MULTILANGUAGE_PROVIDER (ID, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(COUNTRY_AREA_TYPE_NAME_ID, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);
	insert into MULTILANGUAGE (ID, LABEL, LANGUAGE_ID, MULTILANGUAGE_PROVIDER_ID, START_DATE, END_DATE, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE) values(SEQ_MULTILANGUAGE.NEXTVAL, 'Belgium', 4, COUNTRY_AREA_TYPE_NAME_ID, SYSDATE, null, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);

	insert into AREA (ID, FUNCTIONAL_ID,  TYPE, NAME_PROVIDER_ID, CODE, START_DATE, END_DATE, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE, CREATION_DATE_SOURCE, CREATION_USER_SOURCE) values(AREA_ID, 'COUNTRY_NIS_150', SEQ_AREA_TYPE.CURRVAL, COUNTRY_AREA_TYPE_NAME_ID, 'COUNTRY', SYSDATE, SYSDATE, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE, SYSDATE, 'noPrincipal');
	insert into COUNTRY (ID, ISO31661NUMCODE, ISO31661ALPHA2CODE, ISO31661ALPHA3CODE) values(AREA_ID, 56, 'BE', 'BEL');
	COMMIT;
END;
GO

