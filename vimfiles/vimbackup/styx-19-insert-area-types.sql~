    ( INCLUDED_ID, 'CITY', AREA_TYPE_NAME_ID, AREA_TYPE_DESCRIPTION_ID, 1, 'insert', SYSDATE, 'update', SYSDATE);

    INSERT INTO AREA_TYPE_LINK (HOLDER_ID, INCLUDED_ID, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE)
    VALUES
    (HOLDER_ID, INCLUDED_ID, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);

	-- INSERT STREET AREA TYPE
    AREA_TYPE_NAME_ID := SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL;
    AREA_TYPE_DESCRIPTION_ID := SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL;
    HOLDER_ID := INCLUDED_ID;
    INCLUDED_ID := SEQ_AREA_TYPE.NEXTVAL;

