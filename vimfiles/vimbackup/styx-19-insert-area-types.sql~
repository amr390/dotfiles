DECLARE
    AREA_TYPE_NAME_ID NUMBER;
    AREA_TYPE_DESCRIPTION_ID NUMBER;
    HOLDER_AT_ID NUMBER;
    INCLUDED_AT_ID NUMBER;
BEGIN
    SELECT SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL INTO AREA_TYPE_NAME_ID FROM DUAL;
    SELECT SEQ_MULTILANGUAGE_PROVIDER.NEXTVAL INTO AREA_TYPE_DESCRIPTION_ID FROM DUAL;
    SELECT ID INTO HOLDER_ID FROM AREA_TYPE WHERE CODE = 'COUNTRY';
    INCLUDED_ID := SEQ_AREA_TYPE.NEXTVAL;

    -- INSERT CITY AREA TYPE
    INSERT INTO MULTILANGUAGE_PROVIDER
    ( ID, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE)
    VALUES
    ( AREA_TYPE_NAME_ID, 1, 'noPrincipal', SYSDATE, 'noPrincipal', SYSDATE);

    INSERT INTO MULTILANGUAGE
    ( ID, LABEL, LANGUAGE_ID, MULTILANGUAGE_PROVIDER_ID, START_DATE, END_DATE, VERSION, USER_ADD, DT_ADD, USER_UPDATE, DT_UPDATE)
