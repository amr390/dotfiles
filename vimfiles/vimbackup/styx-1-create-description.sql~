CREATE TABLE MULTILANGUAGE(
    ID NUMBER(20,0) NOT NULL, 
    LABEL VARCHAR2(255) NOT NULL,
    LANGUAGE_ID NUMBER(20,0) NOT NULL,
    MULTILANGUAGE_PROVIDER_ID NUMBER(20,0) NOT NULL,
    -- AREA_TYPE_MULTILANGUAGE_ID NUMBER(20,0),
    -- AREA_TYPE_NAME_ID NUMBER(20,0),
    -- AREA_NAME_ID NUMBER(20,0),
    START_DATE DATE,
    END_DATE DATE
   ) TABLESPACE STYX_TABLES; 

CREATE SEQUENCE SEQ_MULTILANGUAGE;







--primary key
ALTER TABLE MULTILANGUAGE ADD CONSTRAINT PK_MULTILANGUAGE_ID PRIMARY KEY (ID)
USING INDEX TABLESPACE STYX_INDEXES;

ALTER TABLE MULTILANGUAGE ADD CONSTRAINT FK_MULTILANGUAGE_PROVIDER FOREIGN KEY (MULTILANGUAGE_PROVIDER_ID) 
REFERENCES MULTILANGUAGE_PROVIDER(ID);


