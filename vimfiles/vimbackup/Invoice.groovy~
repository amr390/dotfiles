package com.cucharitas.sat.domain


class Invoice {

    String invoiceType
    String nfra
    Date dateCreated
    Date lastUpdated
    Date registerDate
    int amount
    int ivaType
    float ivaCost
    int irpfType
    float irpfCost
    float baseCost
    float totalCost


    static belongsTo = [product:Product, provider:Provider]


    static constraints = {
        invoiceType inList:["compra", "venta"]
        nfra() //unique: true, matches: /^([A-Z]{3})\/(\d{2})\/(\d+)$/
        registerDate()
        dateCreated()
        lastUpdated()
        ivaType()
        ivaCost()
        irpfType()
        irpfCost()
        baseCost()
        totalCost()
    }
}
