DECLARE
	BELGIUM_AREA_ID NUMBER;
	NISCODE_ATTRIBUTE_TYPE NUMBER;
BEGIN
    	SELECT ID INTO BELGIUM_AREA_ID FROM COUNTRY C WHERE C.ISO31661NUMCODE = 56;

	SELECT ID INTO NISCODE_ATTRIBUTE_TYPE FROM ATTRIBUTE_TYPE AT WHERE AT.CODE = 'COUNTRY_NISCODE';
	insert into ATTRIBUTE (ID, DT_ADD, USER_ADD, DT_UPDATE, USER_UPDATE, VALUE, VERSION, AREA_ID, TYPE_ID) values(SEQ_ATTRIBUTE.NEXTVAL, SYSDATE, 'noPrincipal', SYSDATE, 'noPrincipal', '150', 1, BELGIUM_AREA_ID, NISCODE_ATTRIBUTE_TYPE);


	COMMIT;
END;
GO

