DECLARE
CRAB_SOURCE_ID NUMBER(20,0);
BEGIN
    SELECT id INTO CRAB_SOURCE_ID FROM SOURCE WHERE CODE = 'CRAB';


    UPDATE ATTRIBUTE_TYPE SET SOURCE_ID=CRAB_SOURCE_ID
    WHERE CODE IN ('COUNTRY_NISCODE', 'CITY_NISCODE','CITY_CRAB_ID',  'STREET_CRAB_ID', 'HOUSENUMBER_CRAB_ID', 'POSTALCODE_CRAB_ID');

    COMMIT;

END;
GO
