(function() {
    'use strict';

    angular.module('vatbook.services')
    .service('PrimitiveManagerService', ['$http', function($http){

        this.getAllWithholdingTaxes = function(){
            var response,
            result;
            response =  $http.get('/sat-grails/report/withholdingtaxes').success(function(data){
                return data;
            })
            .error(function(data){
                return data;
            });

            return response;
        };

        this.getWithholdingForProvider = function(id){
            var response;
            $https.get('sat-grails/report/withholdingtaxes/' + id).success(function(data){
                response = data;
                console.log(data);
            })
            .error(function(data){
                response = data;
                console.log(data);
            });

            return response;
        };
    }]);
}());
